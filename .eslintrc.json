{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "next/core-web-vitals, eslint:recommended",
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-const-assign": "error",
    "no-empty": "warn",
    "no-mixed-spaces-and-tabs": "warn"
  }
}
